@using SpotMarket.WebAssembly.Models.Presentation

@if (Data != null)
{
    <div class="supplier-header-container">
        <div class="header-icon">
            <i class="bi bi-buildings-fill"></i>
        </div>
        <div class="header-info">
            <h1 class="supplier-name">@Data.SupplierName</h1>
            <h2 class="page-title">تحلیل عملکرد عرضه‌کننده</h2>
        </div>
    </div>
}

@code {
    [Parameter]
    public SupplierHeaderData? Data { get; set; }
}
